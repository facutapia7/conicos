<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN" "http://castor.exolab.org/mapping.dtd">

<mapping>

  <description>A mapping file for our Config application</description>
 
  <class name="ar.com.osde.cronicos.beans.AfiliadoBCC">

    <map-to xml="DATOSSOCIO"/>

    <field name="Filial" type="string">
      <bind-xml name="FILIAL" node="element"/>
    </field>

    <field name="NumeroSocio" type="string">
      <bind-xml name="NROSOC" node="element"/>
    </field>

    <field name="Orden" type="string">
      <bind-xml name="ORDEN" node="element"/>
    </field>

  </class>
  
</mapping>