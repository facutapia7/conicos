    </field>

    <field name="NumeroSocio" type="string">
      <bind-xml name="NROSOC" node="element"/>
    </field>

    <field name="Orden" type="string">
      <bind-xml name="ORDEN" node="element"/>
    </field>

    <field name="Nombre" type="string">
      <bind-xml name="NAME_FIRST" node="element"/>
    </field>

    <field name="Apellido" type="string">
      <bind-xml name="NAME_LAST" node="element"/>
    </field>
    
    <field name="Fnac" type="string">
      <bind-xml name="BIRTHDT" node="element"/>
    </field>
    
    <field name="Sexo" type="string">
      <bind-xml name="SEXO" node="element"/>
    </field>

    <field name="TpoEsp" type="string">
      <bind-xml name="TPOESP" node="element"/>
    </field>

    <field name="Parentesco" type="string">
      <bind-xml name="RELFAM" node="element"/>
