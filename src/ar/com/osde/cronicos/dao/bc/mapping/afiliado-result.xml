    <field name="FechaAlta" type="string">
      <bind-xml name="FECALT" node="element"/>
    </field>

    <field name="Fecbaj" type="string">
      <bind-xml name="FECBAJ" node="element"/>
    </field>

    <field name="TipSoc" type="string">
      <bind-xml name="TIPSOC" node="element"/>
    </field>

    <field name="DescrTipsoc" type="string">
      <bind-xml name="DESC_TIPSOC" node="element"/>
    </field>

    <field name="PlanServ" type="string">
      <bind-xml name="PLAN" node="element"/>
    </field>

    <field name="MesesReconocidos" type="string">
      <bind-xml name="MESREC" node="element"/>
    </field>

    <field name="NumeroIC" type="string">
      <bind-xml name="IC_GF" node="element"/>
    </field>

  </class>

</mapping>